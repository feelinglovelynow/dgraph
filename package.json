{
  "name": "@feelinglovelynow/dgraph",
  "version": "1.1.0",
  "license": "MIT",
  "type": "module",
  "types": "./dist/index.ts",
  "exports": "./dist/index.js",
  "description": "Features a DgraphTransaction class that creates a transaction object with lots of features to ease workflow with dgraph cloud instances.",
  "author": "https://github.com/feelinglovelynow?tab=repositories",
  "files": [
    "dist",
    "tsc",
    "src"
  ],
  "keywords": [
    "dgraph",
    "graph",
    "database",
    "edge",
    "cloud",
    "svelte",
    "react"
  ],
  "scripts": {
    "clear": "rm -rf ./dist ./tsc",
    "createTypes": "pnpm tsc",
    "minify": "chmod +x build.cjs && ./build.cjs",
    "copy": "cp ./src/index.ts ./dist/index.ts",
    "build": "pnpm clear && pnpm createTypes && pnpm minify && pnpm copy"
  },
  "devDependencies": {
    "esbuild": "^0.19.7",
    "typescript": "^5.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/feelinglovelynow/dgraph.git"
  },
  "bugs": {
    "url": "https://github.com/feelinglovelynow/dgraph/issues"
  }
}
